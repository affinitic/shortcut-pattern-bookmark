                "id": "homepage"
            },
            {
                "name": "ZMI Root",
                "pattern": "{%start%}/manage_main",
                "id": "zmi_root"
            },
            {
                "name": "ZMI in place",
                "pattern": "{%start%}{%path%}/manage_main",
                "id": "zmi_in_place"
            },
            {
                "name": "Viewlet Manager",
                "pattern": "{%start%}{%path%}/@@manage-viewlets",
                "id": "viewlet_manager"
            },
            {
                "name": "Portal Catalog",
                "pattern": "{%start%}/portal_catalog/manage_main",
                "id": "portal_catalog"
            },
            {
                "name": "Disable Diazo",
                "pattern": "{%start%}{%path%}{%query%}?diazo.off=1",
                "query": true,
                "id": "disable_diazo"
            },
            {
                "name": "Catalog portal type",
                "pattern": "{%start%}/portal_catalog/Indexes/portal_type/manage_browse",
                "id": "catalog_portal_type"
            },
            {
                "name": "Export Content",
                "pattern": "{%start%}{%path%}/@@export_content",
                "id": "export_content"
            },
            {
                "name": "Control Panel",
                "pattern": "{%start%}/@@overview-controlpanel",
                "id": "control_panel"
            },
            {
                "name": "Upgrade step",
                "pattern": "{%start%}/portal_setup/manage_upgrades",
                "id": "upgrade_step"
            },
            {
                "name": "Registry",
                "pattern": "{%start%}/portal_registry",
                "id": "registry"
            },
            {
                "name": "Modules",
                "pattern": "{%start%}/prefs_install_products_form",
                "id": "modules"
            },
            {
                "name": "Folder Content",
                "pattern": "{%start%}{%path%}/folder_contents",
                "id": "folder_content"
            },
            {
                "name": "Undo",
                "pattern": "{%start%}/Control_Panel/Database/main/manage_UndoForm",
                "id": "undo"
            },
            {
                "id": "undo_(plone_4)",
                "name": "Undo (Plone 4)",
                "pattern": "{%start%}/manage_UndoForm"
            },
            {
                "id": "History",
                "name": "History",
                "pattern": "{%start%}/{%path%}/@@historyview"
            },
            {
                "id": "Login",
                "name": "Login",
                "pattern": "{%start%}/login"
            }
        ]
    },
    "ports": {
        "action": "set",
        "items" : [
            {
                "ports": "8080",
                "siteRoots": ["plone", "Plone"]
            },
            {
                "ports": "3000",
                "siteRoots": ["{%no_site_root%}"]
            }
        ]
    }
}
